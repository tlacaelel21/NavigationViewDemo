<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vinidsl.navigationviewdemo" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:name="GlobalClass"
        android:icon="@drawable/ico57"
        android:label="@string/app_name"
        android:theme="@style/Theme.NavigationViewDemo"
        >

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleInstance"
            android:screenOrientation="nosensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".NoticiasActivity"
            android:label="@string/noticia_cab"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".NoticiaActivity"
            android:label="@string/noticia_cab"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".PatrocinadoresActivity"
            android:label="@string/patrocinadores_cab"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".PonentesActivity"
            android:label="@string/ponentes_cab"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".PonenteActivity"
            android:label="@string/ponentes_cab"
            android:parentActivityName=".PonentesActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PonentesActivity" />
        </activity>

        <activity
            android:name=".EncuestaActivity"
            android:label="@string/encuestas_cab"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".ProgramaActivity"
            android:label="@string/programa_cab"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>

        <activity
            android:name=".Evento"
            android:screenOrientation="nosensor"
            android:label="@string/evento_cab"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>


    </application>

</manifest>
